<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        id="WebApp_ID" version="3.0">
    <display-name>pager</display-name>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>SublistCourseServlet</servlet-name>
        <servlet-class>com.classba.center.servlet.SublistCourseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SublistCourseServlet</servlet-name>
        <url-pattern>/sublist/SublistServlet</url-pattern>
    </servlet-mapping>
</web-app>